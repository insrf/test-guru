<%= form_with model: [:admin, @test], local: true do |form| %>
  <%= render 'shared/errors', resource: @test %>

  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p><br>

  <p>
    <%= form.label :level %><br>
    <%= form.text_field :level %>
  </p><br>

  <p>
    <%= form.label :category_id %><br>
    <%= form.text_field :category_id %>
  </p><br>



<p><%= form.submit %></p>
<% end %>
