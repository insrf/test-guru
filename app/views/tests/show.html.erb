<h1>Tests</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th>Quantity Questions</th>
    <th>Action</th>
  </tr>

  <tr>
    <td><%= @test.title %></td>
    <td><%= test_level(@test) %></td>
    <td><%= @test.questions.count %></td>
    <td><%= link_to 'View', test_questions_path(test_id: params[:id])%></td>
  </tr>
</table>
