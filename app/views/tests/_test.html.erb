<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td class="align-middle"><%= test.timer_on %></td>
  <% if test.timer_on == true %>
  <td class="align-middle"><%= test.timer.strftime("%H:%M") %></td>
  <% else %>
  <td class="align-middle">Without timer</td>
  <% end %>
  <td>
  <%= button_to t('.start'), start_test_path(test), class: "btn btn-primary" %>
  </td>
</tr>
